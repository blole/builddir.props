<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>builddir</id>
		<version>3.2.1</version>
		<authors>blole</authors>
		<owners>blole</owners>
		<projectUrl>https://github.com/blole/visual-studio-builddir</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>build projects in a separate output directory</description>
		<tags>native nativepackage C++</tags>
		<dependencies>
			<dependency id="manage-property-sheets" version="[1,2)" />
		</dependencies>
		<developmentDependency>true</developmentDependency>
	</metadata>
	<files>
		<file src="dummy.hpp" target="lib\native" />
		<file src="install.ps1" target="tools" />
		<file src="uninstall.ps1" target="tools" />
		<file src="..\builddir.props" target="tools" />
		<file src="..\package.xml" target="tools" />
	</files>
</package>
